generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  userId    Int      @id @default(autoincrement()) @map("id_user")
  username  String
  email     String
  password  String
	language 	Language @default(en)
  createdAt DateTime @default(now()) @map("created_at")
}

enum Language {
	fr
	en
}
